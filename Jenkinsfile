pipeline {
  agent any
    stages {
        stage('word') {
            steps {
                echo 'Hello World'
            }
        }
      stages {
        stage('sharath') {
            steps {
                echo 'Hello sharath'
            }
        }
        stages {
        stage('manoj') {
            steps {
                echo 'Hello manoj'
            }
        }
    }
}
